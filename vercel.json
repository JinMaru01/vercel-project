{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "installCommand": "yarn install",
  "buildCommand": "yarn build",
  "devCommand": "yarn dev",
  "framework": "nextjs"
}
